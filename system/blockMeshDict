FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.1; //mm -> cm

vertices
(
  (0 -1  0.5)
  (1 -1  0.5)
  (1 -1 -0.5)
  (0 -1 -0.5)
  (0  1  0.5)
  (1  1  0.5)
  (1  1 -0.5)
  (0  1 -0.5)//7
);

blocks
(                       //(x z y)
    hex (0 1 2 3 4 5 6 7) (1 1 8) simpleGrading (1 1 1)
);

edges
(
);

boundary
(

     inlet
     {
        type patch;
        faces
        (
            (4 7 3 0)
        );
    }

    outlet
    {
        type patch;
        faces
        (
            (6 5 1 2)
        );
    }

    walls
    {
        type wall;
        faces
        (
            (4 5 6 7)
            (3 2 1 0)
        );
    }
    
   frontAndBack
    {
        type empty;
        faces
        (
            (7 6 2 3)
            (5 4 0 1)
        );
    }   
);

mergePatchPairs
(
);

