  (0  1 -0.25)
  (0  0 -0.25)
  (15 0 -0.25)
  (30 0 -0.25)
  (30 1 -0.25)//15
);

blocks
(
    hex (0 1 2 3 8 9 10 11) (300 60 1) simpleGrading (1 1 1)
    hex (3 2 5 4 11 10 13 12) (300 20 1) simpleGrading (1 1 1)
