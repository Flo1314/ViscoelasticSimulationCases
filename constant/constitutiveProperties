FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      constitutiveProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

parameters
{               
                type             Oldroyd-BLog;

                rho              rho [1 -3 0 0 0 0 0] 1000.0; // 1000 kg/m² = 1 g/cm³
                etaS             etaS [1 -1 -1 0 0 0 0] 18000.0;
                etaP             etaP [1 -1 -1 0 0 0 0] 2000.0;               
                lambda           lambda [0 0 1 0 0 0 0] 0.3;

                stabilization    coupling;              
}

passiveScalarProperties
{
                solvePassiveScalar   no;
                D                    D [ 0 2 -1 0 0 0 0 ] 1e-9; 
}

